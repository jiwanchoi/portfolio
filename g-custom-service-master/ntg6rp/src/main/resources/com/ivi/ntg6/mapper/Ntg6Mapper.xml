		  		 ,#{GMSTAT}
		  		 ,#{REQST}
		  		 ,#{REQDT}
		  		 ,#{REQWPCNT}
		  		 ,#{REQAVCNT}
		  		 ,#{RES_TDIS}
		  		 ,#{RES_TTOL}
		  		 ,#{RES_TDUR}
		  		 ,#{RES_TRDUR}
		  		 ,#{GCERCODE}
		  		 ,#{GCTRYCNT}
		  		 ,#{RES_CNT}
		  		 ,#{RES_ROUTE_TYPE}
		  		 ,#{maxElapsedTime}
		  		 ,#{avgElapsedTime}
		  		 ,#{minElapsedTime}
    		)
    	</insert>
    	
    	
    	
</mapper>